import java.util.LinkedList;
public class Main
{
	public static void main(String[] args) {
		LinkedList<Integer> numeros = new LinkedList<>();
	 numeros.add(10);
	 numeros.add(20);
     numeros.add(30);
     numeros.add(40);
     numeros.add(50);
     int soma = 0;
     for (int i = 0; i < numeros.size(); i++){
         soma = soma + numeros.get(i);
     }
     System.out.println("soma Ã© " + soma);
	}
}
